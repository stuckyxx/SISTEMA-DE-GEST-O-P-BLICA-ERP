{
    "rewrites": [
        {
            "source": "/api-admin/:path*",
            "destination": "https://backgestao.pythonanywhere.com/:path*"
        },
        {
            "source": "/api-client/:subdomain/:path*",
            "destination": "https://:subdomain.pythonanywhere.com/:path*"
        },
        {
            "source": "/api/:path*",
            "destination": "https://backgestao.pythonanywhere.com/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}